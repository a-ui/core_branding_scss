{
  "name": "@a-ui/core",
  "version": "6.4.0",
  "description": "core_branding_scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-ui/core_branding_scss.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf tmp",
    "clean:docs": "rimraf docs",
    "copy-package": "run-p copy-package:*",
    "copy-package:styles": "copyfiles -u 1 \"src/styles/**/*\" -e \"src/styles/**/styleguide.scss\" dist/assets",
    "copy-package:icons": "copyfiles -u 1 \"src/icons/**/*\" dist/assets",
    "copy-package:images": "copyfiles -u 1 \"src/images/**/*\" dist/assets",
    "copy-package:fonts": "copyfiles -u 1 \"src/fonts/**/*\" dist/assets",
    "copy-docs": "run-p copy-docs:*",
    "copy-docs:images": "copyfiles -u 1 \"src/images/**/*\" docs2",
    "copy-docs:fonts": "copyfiles -u 1 \"src/fonts/**/*\" docs2",
    "copy-docs:scripts": "copyfiles -u 1 \"src/scripts/**/*\" docs2",
    "copy-docs:html": "copyfiles -u 1 \".tmp/**/*.html\" docs2",
    "gulp-build": "gulp build",
    "gulp-start": "gulp server",
    "gulp-docs": "gulp docs",
    "gulp-cdn": "gulp cdn",
    "gulp-version": "gulp-docs && git add -A .",
    "build": "npm-run-all clean:dist gulp-build",
    "start": "npm-run-all clean:tmp gulp-start",
    "docs": "npm-run-all clean:docs gulp-docs",
    "aws": "npm-run-all clean:dist gulp-build gulp-cdn",
    "version": "npm-run-all gulp-version"
  },
  "contributors": [
    {
      "name": "Jasper Van Proeyen",
      "email": "jasper.vanproeyen@digipolis.be"
    },
    {
      "name": "Sander Geenen",
      "email": "sander.geenen@studiohyperdrive.be"
    }
  ],
  "license": "MIT",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.1",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.15",
    "glob": "^7.2.3",
    "gulp": "^4.0.2",
    "gulp-header-license": "^1.0.12",
    "gulp-nunjucks": "^5.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-svg-sprite": "^2.0.3",
    "gulp-watch": "^5.0.1",
    "list-selectors": "^2.0.1",
    "merge-stream": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "sass-export": "^2.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "yargs": "^13.3.2"
  }
}
