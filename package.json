{
  "name": "@a-ui/core",
  "version": "6.4.1",
  "description": "core_branding_scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-ui/core_branding_scss.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf tmp",
    "clean:docs": "rimraf docs",
    "copy:dist": "run-p copy-dist:*",
    "copy:docs": "run-p copy-docs:*",
    "sass:tmp": "run-s sass-tmp:scss sass-tmp:prefix",
    "sass:docs": "run-s sass-docs:scss sass-tmp:prefix",
    "copy-dist:styles": "copyfiles -u 1 \"src/styles/**/*\" -e \"src/styles/**/styleguide.scss\" dist/assets",
    "copy-dist:icons": "copyfiles -u 1 \"src/icons/**/*\" dist/assets",
    "copy-dist:images": "copyfiles -u 1 \"src/images/**/*\" dist/assets",
    "copy-dist:fonts": "copyfiles -u 1 \"src/fonts/**/*\" dist/assets",
    "copy-docs:styles": "copyfiles -u 1 \".tmp/styles/main.css\" \".tmp/styles/styleguide.css\" docs",
    "copy-docs:images": "copyfiles -u 1 \"src/images/**/*\" docs",
    "copy-docs:fonts": "copyfiles -u 1 \"src/fonts/**/*\" docs",
    "copy-docs:scripts": "copyfiles -u 1 \"src/scripts/**/*\" docs",
    "copy-docs:html": "copyfiles -u 1 \".tmp/**/*.html\" docs",
    "sass-tmp:scss": "sass src/styles:.tmp/styles --no-source-map --style=expanded --watch",
    "sass-docs:scss": "sass src/styles:.tmp/styles --no-source-map --style=expanded",
    "sass-tmp:prefix": "postcss -u autoprefixer -r --no-map .tmp/styles/*.css",
    "stylelint": "stylelint \"src/styles/**/*.scss\" --formatter verbose",
    "watch:scss": "run-s sass:tmp",
    "watch:njk": "onchange 'src/**/*.njk' -- npm run gulp-templates",
    "server": "browser-sync start -s .tmp -s src -f='.tmp/**/*.css' -f='.tmp/index.html' --no-notify --port 9000",
    "gulp-build": "gulp build",
    "gulp-docs": "gulp docs",
    "gulp-cdn": "gulp cdn",
    "gulp-version": "gulp-docs && git add -A .",
    "gulp-iconsprite": "gulp icon-sprite",
    "gulp-templates": "gulp templates",
    "build": "npm-run-all clean:dist gulp-iconsprite gulp-build copy:dist",
    "docs": "npm-run-all clean:docs gulp-iconsprite sass:docs gulp-docs copy:docs",
    "aws": "npm-run-all clean:dist gulp-build gulp-cdn",
    "version": "npm-run-all gulp-version",
    "start": "run-p clean:tmp gulp-iconsprite stylelint watch:scss watch:njk server"
  },
  "contributors": [
    {
      "name": "Jasper Van Proeyen",
      "email": "jasper.vanproeyen@digipolis.be"
    },
    {
      "name": "Sander Geenen",
      "email": "sander.geenen@studiohyperdrive.be"
    }
  ],
  "license": "MIT",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.15",
    "glob": "^7.2.3",
    "gulp": "^4.0.2",
    "gulp-header-license": "^1.0.12",
    "gulp-nunjucks": "^5.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^2.0.3",
    "list-selectors": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "sass-export": "^2.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0"
  }
}
